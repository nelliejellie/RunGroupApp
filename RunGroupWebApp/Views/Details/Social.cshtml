@model SocialDetail

<h2>Social Details</h2>

<form asp-action="Social" method="post">
    <div class="form-group">
        <label for="Email">Email</label>
        <input type="email" class="form-control" id="Email" name="Email" asp-for="@Model.Email">
    </div>
    <div class="form-group">
        <label for="Phone">Phone</label>
        <input type="text" class="form-control" id="Phone" name="Phone" asp-for="@Model.Phone">
    </div>
    <button type="submit" class="btn btn-primary">Next</button>
</form>